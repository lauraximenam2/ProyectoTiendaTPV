<Application x:Class="ProyectoTiendaTPV.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProyectoTiendaTPV"
             xmlns:converters="clr-namespace:ProyectoTiendaTPV.Converters"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:ImagePathToBitmapConverter x:Key="ImagePathConverter"/>
        <converters:TicketItemSubtotalConverter x:Key="TicketItemSubtotalConverter"/>
        <converters:EqualityConverter x:Key="EqualityConverter"/>
        <converters:StockToBooleanConverter x:Key="StockToBooleanConverter"/>
    </Application.Resources>
</Application>